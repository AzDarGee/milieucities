{
  "name": "milieu-client",
  "version": "1.0.0",
  "description": "Milieu client directory",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run build:prod || npm run start:dev",
    "build:prod": "npm run clean && npm run prod && npm run build:dll && npm run build:webpack",
    "postinstall": "postinstall": "npm run clean && npm run prod && npm run build:dll && npm run build:webpack",
    "prod": "NODE_ENV=production PORT=3000 node server.js",
    "dev": "NODE_ENV=development PORT=8090 node server.js",
    "start:dev": "npm run build:dll && npm run dev && webpack --config webpack.dev.config.js --watch --progress",
    "heroku-postbuild": "webpack -p --config webpack.prod.config.js --progress",
    "clean": "rimraf dist",
    "build:webpack": "webpack --config webpack.prod.config.js",
    "build:dll": "webpack --config config.dll.js",
    "build": "npm run clean && npm run build:dll && npm run build:webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Milieucitiesrepo/m-server.git"
  },
  "author": "Milieucitiesrepo",
  "dependencies": {
    "antd": "^2.12.1",
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-lodash": "^3.1.4",
    "babel-preset-react": "^6.24.1",
    "big.js": "^3.1.3",
    "chart.js": "^2.3.0",
    "compile": "0.0.2",
    "cors": "^2.8.4",
    "css-loader": "^0.25.0",
    "es6-promise": "^3.2.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.6.1",
    "font-awesome-webpack": "0.0.4",
    "if-env": "1.0.0",
    "image-webpack-loader": "^1.8.0",
    "immutable": "^3.8.1",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "loader-utils": "^1.1.0",
    "lodash": "^4.13.1",
    "moment": "^2.17.1",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "rc-collapse": "^1.6.12",
    "react": "^15.2.1",
    "react-collapsible": "^1.2.1",
    "react-datepicker": "^0.40.0",
    "react-dom": "^15.2.1",
    "react-localization": "^0.0.8",
    "react-map-gl": {
      "dependencies": {
        "gulp-sourcemaps": "^1.7.0"
      },
      "version": "^1.2.0"
    },
    "react-share": "^1.11.0",
    "react-tabs": "^1.0.0",
    "riek": "^1.0.7",
    "rimraf": "^2.6.1",
    "sass-loader": "^3.2.0",
    "sass-resources-loader": "^1.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "velocity-animate": "^1.2.3",
    "webpack": "^1.15.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "express": "^4.15.3",
    "http-server": "^0.8.5",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "webpack-dev-server": "^1.14.1"
  }
}
